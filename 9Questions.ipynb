def permute(s):
   
    if len(s) == 1:
        return [s]
    
   
    permutations = []
    
   
    for i in range(len(s)):
        
        current_char = s[i]
        
        
        remaining_chars = s[:i] + s[i+1:]
        
        
        for perm in permute(remaining_chars):
            
            permutations.append(current_char + perm)
    
    return permutations


string = "ABC"
result = permute(string)


print("All permutations of the string:", result)
